{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/config": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dependency-injection": "7.4.*",
        "symfony/filesystem": "7.4.*",
        "symfony/process": "7.4.*"
    },
    "require-dev": {},
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
